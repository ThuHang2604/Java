    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>hang.servlet.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>hang.servlet.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>hang.servlet.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowResultController</servlet-name>
        <servlet-class>hang.servlet.ShowResultController</servlet-class>
